{
  "manifest_version": 3,
  "name": "Scroll Toll",
  "version": "1.0.0",
  "description": "Chrome extension with React",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}